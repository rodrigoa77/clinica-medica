<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>br.edu.imepac.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CadastrarFuncionarioServlet</servlet-name>
        <servlet-class>br.edu.imepac.servlets.admin.funcionarios.CadastrarFuncionarioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CadastrarFuncionarioServlet</servlet-name>
        <url-pattern>/pages/admin/funcionarios/cadastrarFuncionarioServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ListagemFuncionarioServlet</servlet-name>
        <servlet-class>br.edu.imepac.servlets.admin.funcionarios.ListagemFuncionarioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListagemFuncionarioServlet</servlet-name>
        <url-pattern>/listagemFuncionarioServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AtualizarFuncionarioServlet</servlet-name>
        <servlet-class>br.edu.imepac.servlets.admin.funcionarios.AtualizarFuncionarioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AtualizarFuncionarioServlet</servlet-name>
        <url-pattern>/atualizarFuncionarioServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>VisualizacaoFuncionarioServlet</servlet-name>
        <servlet-class>br.edu.imepac.servlets.admin.funcionarios.VisualizacaoFuncionarioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VisualizacaoFuncionarioServlet</servlet-name>
        <url-pattern>/visualizacaoFuncionarioServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ExcluirFuncionarioServlet</servlet-name>
        <servlet-class>br.edu.imepac.servlets.admin.funcionarios.ExcluirFuncionarioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ExcluirFuncionarioServlet</servlet-name>
        <url-pattern>/excluirFuncionarioServlet</url-pattern>
    </servlet-mapping>
</web-app>
